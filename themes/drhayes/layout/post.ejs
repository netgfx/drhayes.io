<div class="container">
  <header class="page-header">
    <%- partial('_partial/breadcrumbs') %>
    <%- partial('_partial/date', {display_date: page.date, date_format: 'MMMM DD, YYYY'}) %>
  </header>
  <article class="post">
    <header class="post-header">
      <h2><%= page.title %></h2>
      <%- partial('_partial/date', {display_date: page.date, date_format: 'MMMM DD, YYYY'}) %>
    </header>
    <%- page.content %>
  </article>
</div>
